  "version": "1.0.0",
  "description": "Read files without try catch.",
  "author": "Connor White",
  "license": "MIT",
  "keywords": [
    "read",
    "file",
    "safe",
    "try",
    "catch",
    "ENOENT"
  ],
  "repository": {
